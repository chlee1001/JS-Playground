{
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"],
      "@apis/*": [
        "./src/apis/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@hooks/*": [
        "./src/hooks/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@styles/*": [
        "./src/styles/*"
      ],
      "@utils/*": [
        "./src/utils/*"
      ],
      "@_types/*": [
        "./src/types/*"
      ]
    }
  }
}


